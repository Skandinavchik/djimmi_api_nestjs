generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum Role {
    GUEST
    MEMBER
    ADMIN
}

model User {
    id        Int      @id @default(autoincrement())
    username  String?
    email     String   @unique
    password  String
    role      Role     @default(GUEST)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
